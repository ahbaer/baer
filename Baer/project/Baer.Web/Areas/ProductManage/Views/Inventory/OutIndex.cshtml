@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Index.cshtml";
}

<script>
    var inventoryId = $.request("inventoryId");

    $(function () {
        gridList();
    })

    function gridList() {
        var $gridList = $("#gridList");
        $gridList.dataGrid({
            url: "/ProductManage/Inventory/GetOutGridJson?inventoryId=" + inventoryId,
            height: $(window).height() - 96,
            colModel: [
                { label: "主键", name: "Id", hidden: true, key: true },
                { label: '出库员', name: 'CreatorUserId', width: 150, align: 'left' },
                { label: '出库时间', name: 'CreatorTime', width: 150, align: 'left' },
                { label: '出库重量', name: 'Weight', width: 150, align: 'left' }
            ]
        });
    }
</script>

<div class="gridPanel">
    <table id="gridList"></table>
</div>
